  <thead>
      <h3>ジャンル一覧・追加</h3>
  </thead>


    <tabel>
        <div class='row'>
            <div class='col-md-3'>
            <td class="mt-3">ジャンル名</td>
            </div>
            　 <%= form_with moedel: @genre, local:true do |f| %>
              <td>
                <%= f.text_field :name, class: 'form-control' %>
              </td>
              <td>
                <%= f.submit "新規登録", class: 'btn btn-success' %>
              </td>
             <% end %>
            </div>
        </div>

           <thead>
              <th>ジャンル</th>
               <% @genres.each do |genre| %>
               <tr>
                 <td><%= genre.name %></td>
                 <td><%= link_to edit_admin_genre_path(genre) do %>
                     <td class="btn btn-success">編集する</td>
                     <% end %>
                 </td>
               </tr>
              <% end %>
           </thead>
    </table>