<tabel>
    <thead>
      <div class="col-md-8 offset-md-1">
         ジャンル一覧・追加
      </div>
    </thead>

      <tbody>
         <div class="col-md-8 offset-md-1">
            <td>ジャンル名</td>
               <br>
               <td> <%= form_with model: @genre, url: admin_genres_path, local:true do |f|  %></td>

              <td><%= f.text_field :name, class: 'form-control' %></td>
              <td><%= f.submit "新規登録", class: 'btn btn-success' %></td>
             <% end %>
          </div>
      </tbody>




           <tbody>
              <tr>ジャンル</tr>
               <% @genres.each do |genre| %>
               <tr>
                 <th><%= genre.name %></th>
                 <td><%= link_to edit_admin_genre_path(genre) do %></td>
                     <td class="btn btn-success">編集する</td>

                     <% end %>
               </tr>
              <% end %>
           </tbody>
</table>